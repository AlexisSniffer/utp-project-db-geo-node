{
  "name": "gps-node",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "ts-node ./src/server.ts",
    "build:server": "tsc src/server.ts --outDir build --target ES2017 --module commonjs --esModuleInterop --skipLibCheck",
    "build": "next build && npm run build:server",
    "start": "node build/server.js",
    "lint": "next lint",
    "migrate": "prisma migrate dev --name init",
    "postinstall": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/nextjs-registry": "^1.0.2",
    "@prisma/client": "^6.12.0",
    "antd": "^5.26.1",
    "bcrypt": "^6.0.0",
    "next": "15.3.4",
    "next-auth": "^4.24.11",
    "ol": "^10.6.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.19.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
